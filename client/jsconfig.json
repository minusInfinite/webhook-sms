{
    "compilerOptions": {
        "baseUrl": "./",
        "allowJs": true,
        "checkJs": false,
        "jsx": "react",
        "lib": ["es2020","DOM", "DOM.Iterable"],
        "skipLibCheck": true,
        "module": "ES2020",
        "moduleResolution": "node",
        "preserveSymlinks": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noEmit": true
    },
    "typeAcquisition": {
        "enable": false,
        "include": ["node","react"]
    },
    "exclude": [".yarn","build","**/.yarn"]
}